#include <stdio.h>
#include <math.h>

int main() {

	printf("10.0 : %f\n", round(10.0));
	printf("10.1 : %f\n", round(10.1));
	printf("10.2 : %f\n", round(10.2));
	printf("10.3 : %f\n", round(10.3));
	printf("10.4 : %f\n", round(10.4));
	printf("10.5 : %f\n", round(10.5));
	printf("10.6 : %f\n", round(10.6));
	printf("10.7 : %f\n", round(10.7));
	printf("10.8 : %f\n", round(10.8));
	printf("10.9 : %f\n", round(10.9));
	printf("10.48 : %f\n", round(10.48));

	return 0;
}
